<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Solution1">

    <!-- 솔루션 클릭 시 나타나는 솔루션 가이드_와이어프레임 -->

    <RelativeLayout
        android:id="@+id/some_id"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/some_id"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <!-- 솔루션 몇 주차인지 -->
        <!-- Rectangle 45 -->

        <TextView
            android:id="@+id/review"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="29dp"
            android:layout_marginTop="260dp"
            android:text="오늘 솔루션은 어떠셨나요?" />


        <!-- 솔루션 내용 -->
        <!-- Rectangle 46 -->

        <!-- android:textAppearance="@style/some_id" />-->

        <TextView
            android:id="@+id/some_id1"
            android:layout_width="127dp"
            android:layout_height="22dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="17dp"
            android:layout_marginTop="38dp"
            android:gravity="top"
            android:text="오늘의 솔루션 기록" />

        <View
            android:id="@+id/rectangle_4_2"
            android:layout_width="334.21dp"
            android:layout_height="130dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="330dp"
            android:background="@drawable/rectangle_4" />


        <!-- 솔루션 완료 -->

        <EditText
            android:id="@+id/memo"
            android:layout_width="320dp"
            android:layout_height="110dp"
            android:layout_alignTop="@+id/rectangle_4_2"
            android:layout_marginLeft="38dp"
            android:layout_marginTop="10dp"
            android:background="@null"
            android:gravity="top|start"
            android:hint="솔루션 메모" />

        <Button
            android:id="@+id/btnsave"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rectangle_4_2"
            android:layout_marginStart="22dp"
            android:layout_marginTop="33dp"
            android:background="@drawable/rectangle_4_3"
            android:text="솔루션 완성하기"
            android:textColor="@color/gray"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/feeling"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="25dp"
            android:layout_marginTop="124dp"
            android:text="오늘 느낀 감정은 무엇인가요?" />

        <Button
            android:id="@+id/button1"
            android:layout_width="63dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="18dp"
            android:layout_marginTop="151dp"
            android:text="행복한"
            android:textColor="@color/gray"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button2"
            android:layout_width="63dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="187dp"
            android:layout_marginTop="204dp"
            android:text="불안한"
            android:textColor="@color/gray"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button3"
            android:layout_width="63dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="109dp"
            android:layout_marginTop="204dp"
            android:text="답답한"
            android:textColor="@color/gray"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button4"
            android:layout_width="63dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="18dp"
            android:layout_marginTop="200dp"
            android:text="무기력한"
            android:textColor="@color/gray"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button5"
            android:layout_width="63dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="108dp"
            android:layout_marginTop="152dp"
            android:text="활기찬"
            android:textColor="@color/gray"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button6"
            android:layout_width="63dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="188dp"
            android:layout_marginTop="152dp"
            android:text="무난한"
            android:textColor="@color/gray"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button7"
            android:layout_width="63dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="268dp"
            android:layout_marginTop="150dp"
            android:text="우울한"
            android:textColor="@color/gray"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button8"
            android:layout_width="63dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="269dp"
            android:layout_marginTop="203dp"
            android:text="가벼운"
            android:textColor="@color/gray"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button9"
            android:layout_width="75dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="303dp"
            android:layout_marginTop="281dp"
            android:text="모르겠음"
            android:textColor="@color/gray"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button10"
            android:layout_width="63dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="207dp"
            android:layout_marginTop="281dp"
